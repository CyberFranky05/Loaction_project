# ============================================
# Backend-only Deployment (EC2) Environment Variables
# Frontend is hosted on Vercel separately
# ============================================

# EC2 Public IP (use your actual EC2 public IP)
EC2_PUBLIC_IP=your-ec2-public-ip

# Vercel Frontend URL (for CORS)
VERCEL_FRONTEND_URL=https://your-app.vercel.app

# Port Configuration
BACKEND_PORT=3000
KEYCLOAK_PORT=8080
POSTGRES_PORT=5432
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

# SSL Configuration
SSL_EMAIL=your@email.com

# Database
POSTGRES_PASSWORD=change_this_password

# Keycloak
KEYCLOAK_ADMIN_PASSWORD=change_this_password
KEYCLOAK_CLIENT_SECRET=secret123

# JWT
JWT_SECRET=change_this_jwt_secret

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
SUPABASE_ANON_KEY=your_anon_key_here
